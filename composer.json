{
  "name": "heimrichhannot/contao-news-pagination-bundle",
  "type": "contao-bundle",
  "description": "This bundle offers content pagination for the news reader.",
  "license": "LGPL-3.0+",
  "require": {
    "php": "^7.1",
    "contao/core-bundle": "^4.4",
    "wa72/htmlpagedom": "^1.3",
    "heimrichhannot/contao-head-bundle": "^1.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "phpunit/phpunit": "^4.5",
    "symfony/phpunit-bridge": "^2.8|^3.0"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\NewsPaginationBundle\\": "src/"
    },
    "classmap": [
      "src/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "autoload-dev": {
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\NewsPaginationBundle\\ContaoManager\\Plugin"
  }
}
